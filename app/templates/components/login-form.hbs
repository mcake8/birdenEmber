<div class="module-login-form">
	<div>
		<img src="../img/logo.png">
	</div>
	<div class="form-title">ФОРМА ВХОДА</div>
	<div class="line"></div>
	<div class="wrap">
		<div class="num-1">90
			<div>аниме в базе</div>
		</div>
		<div class="num-3">200
			<div>аниме-серий</div>
		</div>
		<div class="num-2">120
			<div>пользователей</div>
		</div>
	</div>
	<div class="form">
		{{input placeholder="Логин" value=logVal}}
		{{input placeholder="Пароль" value=passVal}}
		<button {{action 'validator'}} disabled={{isDisabled}}>Войти</button>
		<div class="not-now">Пароли и логин не совпадают</div>
	</div>
</div>
